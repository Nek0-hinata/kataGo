<view>
    <van-cell>
        <van-cell is-link center>
            <view style="display: flex; justify-content: space-between;">
                <view id="Avatar">
                    <open-data type="userAvatarUrl"></open-data>
                </view>
                <view id="nickName" style="display: flex; flex-direction: column; justify-content: space-between;">
                    <open-data type="userNickName"></open-data>
                    <view>无等级</view>
                </view>
            </view>
        </van-cell>
    </van-cell>
    <van-cell-group title=" ">
        <van-cell title="余额"
                  is-link
                  icon="gem-o"
                  link-type="navigateTo"
                  url="/pages/index">${{money}}</van-cell>
    </van-cell-group>
    <van-cell-group title=" ">
        <van-cell title="我的星光卡"
                  is-link
                  icon="vip-card-o"
                  link-type="navigateTo"
                  url="/pages/index"></van-cell>
        <van-cell title="我的报告券"
                  is-link
                  icon="label-o"
                  link-type="navigateTo"
                  url="/pages/index"></van-cell>
        <van-cell title="我的道具"
                  is-link
                  icon="gift-o"
                  link-type="navigateTo"
                  url="/pages/index"></van-cell>
        <van-cell title="我的成就"
                  is-link
                  icon="medal-o"
                  link-type="navigateTo"
                  url="/pages/index"></van-cell>
    </van-cell-group>
    <van-cell-group title=" ">
        <van-cell title="帮助"
                  is-link
                  icon="comment-circle-o"
                  link-type="navigateTo"
                  url="/pages/index"></van-cell>
        <van-cell title="关于"
                  is-link
                  icon="info-o"
                  link-type="navigateTo"
                  url="/pages/index">{{version}}</van-cell>
    </van-cell-group>
    <van-cell-group title=" ">
        <van-cell title="设置"
                  is-link
                  icon="setting-o"
                  link-type="navigateTo"
                  url="setting/setting"></van-cell>
    </van-cell-group>
</view>