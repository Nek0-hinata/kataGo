<view>
    <view style="display: flex; justify-content: space-between">
        <view style="display: flex;">
            <view id="Avatar">
                <open-data type="userAvatarUrl"></open-data>
            </view>
            <view style="display: flex; flex-direction: column; justify-content: space-around;">
                <view style="font-size: 14px; position: relative;">
                    <open-data type="userNickName"></open-data>
                    <image src="/img/black.png" class="hei" style="{{blackM}}"></image>
                </view>
                <view style="font-size: 12px">无等级</view>
            </view>
        </view>
        <view style="display: flex;">
            <view style="display: flex; flex-direction: column; justify-content: space-around; align-items: flex-end;">
                <view style="font-size: 14px">
                    <open-data type="userNickName"></open-data>
                </view>
                <view style="font-size: 12px">无等级</view>
            </view>
            <view id="Avatar" style="position: relative;">
                <open-data type="userAvatarUrl"></open-data>
                <image src="/img/white1.png" class="bai" style="{{whiteM}}"></image>
            </view>
        </view>
    </view>
    <canvas id="myCanvas" type="2d" class="Canvas"
    bindtouchstart="start"></canvas>
    <view>Coordinates:({{x}},{{y}})</view>
</view>